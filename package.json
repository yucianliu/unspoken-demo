{
  "name": "unspoken",
  "version": "1.0.0",
  "description": "A simple demo of a React social media app hosted on GitHub Pages",
  "private": true,
  "homepage": "https://yucianliu.github.io/unspoken-demo",
  "dependencies": {
    "class-variance-authority": "^0.7.0",
    "clsx": "^2.0.0",
    "framer-motion": "^12.6.2",
    "lucide-react": "^0.486.0",
    "react": "^18.3.1",
    "react-dom": "^18.0.0",
    "react-scripts": "5.0.1",
    "tailwind-merge": "^2.0.0",
    "web-vitals": "^2.1.4"
  },
  "devDependencies": {
    "@types/node": "^22.13.17",
    "@types/react": "^19.0.12",
    "@types/react-dom": "^19.0.4",
    "gh-pages": "^5.0.0",
    "typescript": "^4.9.5"
  },
  "scripts": {
    "start": "react-scripts start",
    "build": "react-scripts build",
    "test": "react-scripts test",
    "eject": "react-scripts eject",
    "predeploy": "npm run build",
    "deploy": "gh-pages -d build"
